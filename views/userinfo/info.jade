#main
   .container
      section#contact
         .hgroup
            h1 User Information
            h2 This is a user info page.
            ul.breadcrumb.pull-right
               li
                  a(href='/index') Home
                  span.divider /
               li.active Contact
         .row
            .span4.office_address
               address
                  strong Twitter, Inc.
                  br
                  |                          795 Folsom Ave, Suite 600
                  br
                  |                          San Francisco, CA 94107
                  br
                  abbr(title='Phone') P:
                  |  (123) 456-7890
               address
                  strong Full Name
                  br
                  a(href='mailto:#') first.last@example.com
            .span8.contact_form
               form
                  .row
                     .span4
                        label Name
                        input.span4(type='text')
                     .span4
                        label E-mail
                        input.span4(type='text')
                     .span8
                        label Subject
                        input.span8(type='text')
                     .span8
                        label Message
                        textarea.span8(rows='8')
                     .span8
                        a.btn.btn-large.btn-success
                           span Submit Message
